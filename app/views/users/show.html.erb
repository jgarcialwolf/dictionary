<%= button_to "Logout", logout_path, method: :delete, data: { turbo_method: :delete } %>
<h1><%= @user.username %>'s profile</h1>
<h3>saved meanings:</h3>

<ul>
  <% @user.meanings.each do |meaning| %>
    <li>
      <%= meaning.english_word %>: <%= meaning.translation %>
    </li>
  <% end %>
</ul>
